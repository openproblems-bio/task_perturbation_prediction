  if (( ${#VIASH_CHOWN_VARS[@]} )); then
    set +e
    eval docker run --entrypoint=bash -i --rm --shm-size=2g ${VIASH_UNIQUE_MOUNTS[@]} ghcr.io/openproblems-bio/task_perturbation_prediction/methods/lgc_ensemble_predict:1.0.0 -c "'chown $(id -u):$(id -g) --silent --recursive ${VIASH_CHOWN_VARS[@]}'"
    set -e
  fi
}
